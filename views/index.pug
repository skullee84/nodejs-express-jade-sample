doctype html
html
  head
    title= title
    link(rel='stylesheet', href='/public/css/style.css')
  body
    div#container
      h1= message
      ul
        -for(var i=0; i < 5; i++)
          li coding

      ul
        -list = ["Uno", "Dos", "Tres", "Cuatro", "Cinco", "Seis"]
        each item in list
          li(class='custom-li')= item

      ul
        each item in names
          li(class='custom-li')= item
